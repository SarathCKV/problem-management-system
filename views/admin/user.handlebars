<main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
	<table class="table table-bordered">
		<thead>
			<th>Id</th>
			<th>User Type</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Email</th>
		</thead>
		<tbody>
			{{#each user}}
				<tr>
					<td>{{_id}}</td>
					<td>{{userType}}</td>
					<td>{{firstName}}</td>
					<td>{{lastName}}</td>
					<td>{{email}}</td>
					<td><form action="/admin/delete/{{_id}}?_method=DELETE" method="post"><button class="btn btn-outline-danger">Delete</button></form></td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</main>